{
  "name": "rabbit-mq-node",
  "version": "1.0.0",
  "description": "Message sender implemented in nodejs",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "send": "tsc && node ./dist/index.js",
    "server": "ts-node-dev --respawn --transpile-only ./src/server.ts",
    "migrations:create": "typeorm migration:create src/infra/repositories/implementations/db/typeorm/migrations/AlterUsersTable",
    "migrations:run": "tsc && typeorm migration:run -d dist/infra/repositories/implementations/db/typeorm/data-source.js"
  },
  "keywords": [
    "rabbitmq",
    "mq",
    "nodejs"
  ],
  "author": "Raphael Fachim",
  "license": "ISC",
  "devDependencies": {
    "@types/amqplib": "^0.10.1",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/node": "^18.11.18",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "amqplib": "^0.10.3",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "inversify": "^6.0.1",
    "mysql": "^2.18.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.11"
  }
}
